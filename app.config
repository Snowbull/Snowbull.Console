    </connectionStrings>

    <akka>
        <hocon>
            <![CDATA[
                  akka {
                      stdout-loglevel = DEBUG
                      loglevel = DEBUG
                      actor {
                          debug {
                              receive = on
                              autoreceive = on
                              lifecycle = on
                              event-stream = on
                              unhandled = on
                          }
                      }
                }
              ]]>
        </hocon>
    </akka>

